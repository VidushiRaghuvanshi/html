<p id="student"></p>
<p id="student3"></p>
<script>
class student{
    constructor( name,standard)
    {
        this.name=name;
        this.standard= standard;
    }
}
student1=new student("Vidushi",10)
document.getElementById("student").innerHTML ="My name is " +student1.name + " I read in class " + student1.standard;
student3=new student("Jayant",12)
document.getElementById("student3").innerHTML ="My name is " + student3.name + " I read in class " + student3.standard;
</script>
